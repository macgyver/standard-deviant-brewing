{
  "name": "standard-deviant-brewing",
  "version": "0.0.1",
  "description": "Simple homepage for Standard Deviant Brewing",
  "main": "index.html",
  "scripts": {
    "clean-dist": "rm -rf dst/*",
    "make-html": "babel-node src/main.js",
    "make-css": "cp -r css dst/css",
    "make-img": "cp -r img dst/img",
    "dist": "npm run clean-dist; npm run make-html; npm run make-css; npm run make-img"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "6.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.14.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "classnames": "^2.2.5",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "fb-watchman": "^1.9.0",
    "js-beautify": "^1.6.4",
    "node-sass": "^3.8.0",
    "node-sass-json-importer": "^2.1.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  }
}
