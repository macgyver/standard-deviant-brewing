{
  "name": "standard-deviant-brewing",
  "version": "0.0.1",
  "description": "Simple homepage for Standard Deviant Brewing",
  "main": "index.html",
  "scripts": {
    "clean": "rm -rf docs/*",
    "html": "babel-node src/main.js",
    "css": "postcss -c postcss.json -d docs/css css/**/*.css",
    "img": "imagemin img/**/* -o docs/img",
    "dist": "npm run clean; npm run html & npm run css & npm run img; wait"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "6.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.14.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "classnames": "^2.2.5",
    "cssnano": "^3.7.4",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "fb-watchman": "^1.9.0",
    "imagemin-cli": "^3.0.0",
    "js-beautify": "^1.6.4",
    "node-sass": "^3.8.0",
    "node-sass-json-importer": "^2.1.1",
    "normalize.css": "^4.2.0",
    "postcss-cli": "^2.6.0",
    "postcss-combine-duplicated-selectors": "^2.1.1",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  }
}
